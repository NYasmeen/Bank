  <div>
    <div class="row">
      <div class="containers col text-center">
        <h4>Account summary
          </h4>
            <p class="display-4">
              <strong>Rs:</strong> <%= JSON.stringify(Info.account);  %>
              <p>
      </div>
      <div class="containers col">
        <div class="row ">
          <div class="col-md-4 ">

           <h4>Name</h4> 
          </div>
          <div class="col ">


            :       <%= JSON.stringify(Info.name);  %>

          </div>
        </div>
        <div class="row ">
            <div class="col-md-4 ">

            <h4>Email</h4>
          </div>
          <div class="col ">
            :      <%= JSON.stringify(Info.email);  %>

          </div>
        </div>
        <div class="row ">
            <div class="col-md-4 ">
               <h4>Phone Number</h4> 
          </div>
          <div class="col">
            :       <%= JSON.stringify(Info.phonenumber);  %>

          </div>
        </div>
        <div class="row ">
            <div class="col-md-4 ">
               <h4> Address</h4>
          </div>
          <div class="col">
            :       <%= JSON.stringify(Info.address);  %>

          </div>
        </div>
      </div>
    </div>
    <div class="containers ">
        <form class="form-inline" action="/logout" method="GET">
          <button type='submit' class="btn btn-outline-dark my-2 my-sm-0 mx-2" type="button">Withdraw/Deposit</button>
   </form>
   <hr>
   <div>
      Transaction history
   </div>
         
    </div>
