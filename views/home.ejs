    <a class="navbar-brand text-light">
      <%= title %>
    </a>
    <form class="form-inline" action="/logout" method="GET">
      <button type='submit' class="btn btn-outline-white my-2 my-sm-0 mx-2" type="button">Logout</button>
    </form>
  </nav>
